{
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "configuration": {
      "properties": {
        "i18nEjsPreview.defaultLocale": {
          "default": "en",
          "description": "Default locale to use for previews (e.g., 'en', 'fr')",
          "type": "string"
        },
        "i18nEjsPreview.functionName": {
          "default": "__",
          "description": "i18n function name used in templates",
          "type": "string"
        },
        "i18nEjsPreview.i18nFolder": {
          "default": "locales",
          "description": "Folder containing i18n JSON files (relative to workspace root)",
          "type": "string"
        }
      },
      "title": "i18n EJS Preview"
    }
  },
  "description": "Preview i18n translation values inline in EJS templates. Prefill i18n function calls with values from JSON files. Supports multiple locales.Preferences i18n JSON files from a specified folder.",
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "eslint": "^8.x",
    "typescript": "^5.3.0"
  },
  "displayName": "ejs i18n helper",
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "icon.jpeg",
  "license": "MIT",
  "main": "./dist/extension.js",
  "name": "ejs-i18n-helper",
  "publisher": "hippoandco",
  "repository": {
    "type": "github",
    "url": "https://github.com/meatyhippo/i18n-ejs-preview"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "1.0.2"
}